@model Entity.AdminEntity
<div class="col-md-12">
    <div class="card">
        <div class="header">
            <h4 class="title">管理员管理</h4>
        </div>
        <div class="content">
            <form method="post">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label asp-for="adminId" class="control-label">ID</label>
                            <input readonly asp-for="adminId" class="form-control border-input" />
                            <span asp-validation-for="adminId" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label asp-for="account" class="control-label">账号</label>
                            <input readonly asp-for="account" class="form-control border-input" />
                            <span asp-validation-for="account" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label asp-for="name" class="control-label">名称</label>
                            <input asp-for="name" class="form-control border-input" />
                            <span asp-validation-for="name" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label asp-for="phone" class="control-label">手机号</label>
                            <input asp-for="phone" class="form-control border-input" />
                            <span asp-validation-for="phone" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label asp-for="email" class="control-label">邮箱</label>
                            <input asp-for="email" class="form-control border-input" />
                            <span asp-validation-for="email" class="text-danger"></span>
                        </div>
                    </div>
                </div>

                <div class="text-center">
                    <input id="btnSure" type="submit" class="btn btn-info btn-fill btn-wd" value="确 定" />
                    &nbsp;
                    <a asp-action="ModPassword" class="btn btn-danger btn-fill btn-wd" >修改密码</a>
                </div>
                <div class="clearfix"></div>
            </form>
        </div>
    </div>
</div>
<script type="text/javascript">
    	$(document).ready(function(){

    	    $("#liSelf").addClass("active");

    	});
</script>